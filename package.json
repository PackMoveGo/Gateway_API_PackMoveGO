{
  "name": "pack-go-movers-backend",
  "version": "1.0.0",
  "description": "Backend for Pack Go Movers",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:gateway": "nodemon src/gateway.ts",
    "start:prod": "node dist/server.js",
    "start:gateway:prod": "node dist/gateway.js",
    "render:start": "node dist/server.js",
    "dev": "npm run dev:backend",
    "dev:backend": "nodemon src/server.ts",
    "dev:gateway": "nodemon src/gateway.ts",
    "dev:backend:node": "node src/server.ts",
    "build": "npm run build:backend && npm run copy-data",
    "build:backend": "tsc",
    "build:gateway": "tsc src/gateway.ts --outDir dist",
    "copy-data": "cp -r src/data dist/data",
    "postinstall": "npm run build:backend",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:stability": "node test-server-stability.js",
    "test:stability:prod": "TEST_URL=https://api.packmovego.com node test-server-stability.js",
    "test:prod": "node test-production.js",
    "test:security": "node test/test-security.js",
    "test:security:prod": "TEST_URL=https://api.packmovego.com node test/test-security.js",
    "test:load-balancing": "node test-load-balancing.js",
    "test:load-balancing:prod": "API_URL=https://api.packmovego.com node test-load-balancing.js",
    "test:private-service": "node test-private-service.js",
    "test:private-service:prod": "GATEWAY_URL=https://pack-go-movers-gateway.onrender.com PRIVATE_API_URL=https://pack-go-movers-api-private.onrender.com node test-private-service.js",
    "test:uber-features": "node test/test-uber-features.js",
    "test:uber-features:prod": "TEST_URL=https://api.packmovego.com SOCKET_URL=https://api.packmovego.com node test/test-uber-features.js",
    "generate:secrets": "node script/generate-secrets.js",
    "validate:env": "node -e \"require('./dist/config/envValidation').validateEnvironment()\"",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "security:check": "npm run test:security && npm run validate:env",
    "security:setup": "npm run generate:secrets && npm run security:check",
    "clear:visitors": "node clear-visitors.js",
    "visitor:manager": "node visitor-manager.js",
    "clear:running": "node clear-while-running.js"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^2.20.33",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^6.4.14",
    "@types/socket.io": "^3.0.1",
    "@types/ssh2": "^1.15.5",
    "@types/winston": "^2.4.4",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.4.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "mongoose": "^8.16.5",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.9.9",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ssh2": "^1.16.0",
    "stripe": "^14.14.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
