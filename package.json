{
  "name": "pack-go-movers-backend",
  "version": "1.0.0",
  "description": "Backend for Pack Go Movers",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "npm run dev:backend",
    "dev:backend": "nodemon src/server.ts",
    "build": "npm run build:backend && npm run copy-data",
    "build:backend": "tsc",
    "copy-data": "cp -r src/data dist/data",
    "postinstall": "npm run build:backend"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^2.20.33",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^6.4.14",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "nodemailer": "^6.9.9",
    "stripe": "^14.14.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}

